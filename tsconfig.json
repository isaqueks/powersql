{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "rootDir": "./",
      "outDir": "./build",
      "esModuleInterop": true,
      "strict": true,
      "strictNullChecks": false,
      "noImplicitAny": false,
      "noImplicitThis": false,
      "declaration": true,
    },
    "exclude": [
        "routes"
    ],
    "include":[
        "*.ts",
        "!*.d.ts"
    ]
}